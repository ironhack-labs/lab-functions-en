    "import re\n",
    "def check_pass(password):\n",
    "    if len(password) < 8:\n",
    "        return False\n",
    "    if not re.search(r'[a-z]', password):\n",
    "        return False\n",
    "    if not re.search(r'[A-Z]', password):\n",
    "        return False\n",
    "    if not re.search(r'[0-9]', password):\n",
    "        return False\n",
    "    if not re.search(r'[^a-zA-Z0-9]', password):\n",
    "        return False\n",
    "    return True \n",
    "print(check_pass(\"Password123!\")) \n",
    "print(check_pass(\"password\"))      \n",
